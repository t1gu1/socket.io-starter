{
  "name": "socketio-typescript-starter",
  "version": "0.0.1",
  "description": "SocketIO typescript starter",
  "author": "T1gu1Sensei",
  "main": "src/index.js",
  "scripts": {
    "tsc": "tsc",
    "start": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "prod": "tsc && pm2 start --no-daemon dist/index.js",
    "dist": "node dist/index.js"
  },
  "dependencies": {
    "socket.io": "^3.1.1",
    "dotenv": "^8.2.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "pm2": "^4.5.4",
    "typescript": "^4.1.5",
    "ts-node-dev": "^1.1.1",
    "@types/socket.io": "^2.1.13",
    "@types/node": "^14.14.28",
    "@types/axios": "^0.14.0",
    "@types/mongoose": "^5.10.3",
    "@types/sha1": "^1.1.2",
    "@types/uuid": "^8.3.0"
  }
}
